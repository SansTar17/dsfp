package project_01;

public class SearchingAlgorithms {
    
      public void searchParticipant(LinkedList<Participant> targetParticipant) {
        if (head == null) {
            System.out.println("Participant list is empty.");
        } else {
            Node<AnyType> current = head;
            while (current != null) {
                if (current.participant.equals(targetParticipant)) {
                    System.out.println("Participant found: " + targetParticipant);
                    return;
                }
                current = current.next;
            }
            System.out.println("Participant not found: " + targetParticipant);
        }
    }
    
}
